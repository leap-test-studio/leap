    "@mui/icons-material": "^5.15.11",
    "@mui/lab": "^5.0.0-alpha.166",
    "@mui/material": "^5.15.11",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.7",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "history": "^5.3.0",
    "http-proxy-middleware": "^2.0.6",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.6",
    "react": "^18.2.0",
    "react-datepicker": "^6.2.0",
    "react-dom": "^18.2.0",
    "react-loadable": "^5.5.0",
    "react-popper": "^2.3.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.22.2",
    "react-select": "^5.8.0",
    "react-to-print": "^2.15.1",
    "reactflow": "^11.10.4",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
