{
  "name": "studio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' webpack --config config/webpack.prod.js",
    "build:win": "set NODE_OPTIONS='--max-old-space-size=8192' && webpack --config config/webpack.prod.js",
    "build:css": "tailwindcss -i src/assets/css/tailwind.css -o src/assets/css/index.css",
    "clean": "npm run clean:css && npm run clean:js",
    "clean:css": "rimraf ./src/assets/css/index.css",
    "clean:js": "rimraf ./build",
    "dev": "npm run upgrade && npm run prebuild && npm run prettify",
    "prettify": "prettier --ignore-path \"./.prettierignore\" --write \"./**/*.{js,jsx,json,html,ts,tsx}\"",
    "upgrade": "npm-check-updates -u -x *redux* && npm i --force",
    "first-time": "npm i -g npm-check-updates && npm run dev",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prebuild": "npm run clean && npm run build:css",
    "start": "npm run build:css && set NODE_OPTIONS='--max-old-space-size=8192' && webpack serve --config config/webpack.dev.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.18",
    "@jsonforms/core": "3.2.1",
