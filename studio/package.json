{
  "name": "studio",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "NODE_ENV='production' NODE_OPTIONS='--max-old-space-size=4096' webpack --config config/webpack.prod.js",
    "build:win": "set NODE_OPTIONS='--max-old-space-size=4096' && webpack --config config/webpack.prod.js",
    "build:css": "tailwindcss -i src/tailwind.css -o src/index.css",
    "build:css:watch": "echo 'Start Watching tailwindcss' && npm run build:css -- --watch",
    "clean": "npm run clean:css && npm run clean:js",
    "clean:css": "rimraf ./src/index.css",
    "clean:js": "rimraf ./build",
    "dev": "npm run upgrade && npm run prebuild && npm run prettify",
    "prettify": "prettier --ignore-path \"./.prettierignore\" --write \"./**/*.{js,jsx,json,html,ts,tsx}\"",
    "upgrade": "npm-check-updates -u -x react-to-print && npm i --force",
    "first-time": "npm i -g npm-check-updates && npm run dev",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prebuild": "npm run clean && npm run build:css",
    "start": "npm run build:css && set NODE_OPTIONS='--max-old-space-size=2048' && webpack serve --config config/webpack.dev.js",
    "preview": "serve -s build -p 5000",
    "start:preview": "npm run build && npm run preview"
  },
