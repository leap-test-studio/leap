    "webpack-merge": "^5.10.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "prettier"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "quotes": [
        "error"
      ],
      "semi": 1
    }
  },
  "prettier": {
    "printWidth": 150,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "endOfLine": "auto"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
