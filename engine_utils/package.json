{
    "name": "engine_utils",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "node dist/index.js",
        "start:dev": "nodemon --ext js,ts,json,env --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/main.ts",
        "build": "tsc --project tsconfig.build.json",
        "prebuild": "rm -rf tsconfig.build.tsbuildinfo ./dist",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
        "test:coverage": "npm run test -- --coverage",
        "test:ci": "npm run test -- --colors --coverage --ci",
        "lint": "eslint --ext .ts,.js .",
        "prettify": "prettier --ignore-path --ignore-unknown \"./.prettierignore\" --write \"./**/*.{js,jsx,json,html,ts,tsx}\"",
        "upgrade": "npm-check-updates -u && npm i --force"
    },
    "dependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.5",
        "@types/supertest": "^6.0.2",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@jest/types": "^29.6.3",
        "@typescript-eslint/eslint-plugin": "^8.19.1",
        "@typescript-eslint/parser": "^8.19.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.7",
        "eslint": "^9.17.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-tsdoc": "^0.4.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.3.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2"
    }
}
