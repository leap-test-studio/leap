    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "file-stream-rotator": "^1.0.0",
