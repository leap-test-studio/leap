{
  "printWidth": 150,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "none",
  "endOfLine": "auto",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "parser": "mdx"
      }
    }
  ]
}